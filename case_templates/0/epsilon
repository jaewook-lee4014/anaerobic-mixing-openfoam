FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.001;

boundaryField
{
    walls
    {
        type            epsilonWallFunction;
        value           uniform 0.001;
    }
    
    bottom
    {
        type            epsilonWallFunction;
        value           uniform 0.001;
    }
    
    top
    {
        type            inletOutlet;
        inletValue      uniform 0.001;
        value           uniform 0.001;
    }
}

// ************************************************************************* //